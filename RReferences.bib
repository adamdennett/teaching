@article{johnston_population_1969,
	title = {Population movements and metropolitan expansion: {London}, 1960-61},
	shorttitle = {Population movements and metropolitan expansion: {London}, 1960-61},
	url = {http://links.jstor.org/sici?sici=0020-2754%28196903%291%3A0%3A46%3C69%3APMAMEL%3E2.0.CO%3B2-K},
	abstract = {Review of some partial models of intra-urban migration suggests that, in each case, a separate causal mechanism produces virtually the same result. An amalgamation of these models suggests that the majority of in-migrants to a city should first reside in the inner residential areas where their arrival will initiate a process of invasion and succession. Within the suburbs, the greater competition for housing in inner areas should encourage most movers to proceed to destinations closer to the urban periphery, so that the general migratory trend within the city should be away from the centre. These propositions are tested with the data given in the Migration Tables of the 1961 Census for Greater London. This evidence clearly suggests that a process of invasion and succession is occurring within the central part of the area, especially in the west, but farther out, the general expected tendency is not so clear; this is partly, it seems, because of deficiencies in the data. Information on the housing characteristics of migrants further supports the invasion and succession model in the inner areas but, although many of the patterns are as hypothesized, it is not possible definitively to isolate the relevant processes.},
	number = {46},
	journal = {Transactions of the Institute of British Geographers},
	author = {Johnston, R. J.},
