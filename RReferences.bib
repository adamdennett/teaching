	month = jul,
	year = {2014},
	keywords = {career progression, city region, England, human-capital escalator, ONS Longitudinal Study, second-order cities},
	pages = {421--433},
	file = {Full Text PDF:C\:\\Users\\Adam\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\c11x28au.default\\zotero\\storage\\BBV3PWMK\\Champion et al. - 2014 - How Far do England's Second-Order Cities Emulate L.pdf:application/pdf;Snapshot:C\:\\Users\\Adam\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\c11x28au.default\\zotero\\storage\\GX9J7IPM\\Champion et al. - 2014 - How Far do England's Second-Order Cities Emulate L.html:text/html}
